<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Hope.ITMS.Model" assembly="Hope.ITMS.Model" auto-import="false">
    <class name="SysMessage" table="Sys_Message">
        <id name="ID" type="Int32">
            <column name="ID" sql-type="int" />
            <generator class="native"/>
        </id>
        <property name="Receiver" column="Receiver" type="Int32"/>
        <property name="Title" column="Title" type="string"/>
        <property name="Details" column="Details" type="string"/>
        <property name="Status" column="Status" type="Int32"/>
        <property name="ReadTime" column="ReadTime" type="DateTime"/>
        <property name="Creator" column="Creator" type="Int32"/>
        <property name="CreatTime" column="CreatTime" type="DateTime"/>
    </class>
</hibernate-mapping>
