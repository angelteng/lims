<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Hope.ITMS.Model" assembly="Hope.ITMS.Model" auto-import="false">
    <class name="TaskTask" table="Task_Task">
        <id name="ID" type="Int32">
            <column name="ID" sql-type="int" />
            <generator class="native"/>
        </id>
        <property name="Name" column="Name" type="string"/>
        <property name="TypeID" column="TypeID" type="Int32"/>
        <property name="Source" column="Source" type="string"/>
        <property name="Progress" column="Progress" type="Int32"/>
        <property name="Detail" column="Detail" type="string"/>
        <property name="Submitor" column="Submitor" type="Int32"/>
        <property name="SubmitTime" column="SubmitTime" type="DateTime"/>
        <property name="Priority" column="Priority" type="Int32"/>
        <property name="Deadline" column="Deadline" type="DateTime"/>
        <property name="Frequency" column="Frequency" type="Int32"/>
        <property name="Remark" column="Remark" type="string"/>
    </class>
</hibernate-mapping>
