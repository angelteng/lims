<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Hope.ITMS.Model" assembly="Hope.ITMS.Model" auto-import="false">
    <class name="SysLog" table="Sys_Log">
        <id name="ID" type="Int32">
            <column name="ID" sql-type="int" />
            <generator class="native"/>
        </id>
        <property name="Type" column="Type" type="Int32"/>
        <property name="Priority" column="Priority" type="Int32"/>
        <property name="Title" column="Title" type="string"/>
        <property name="Message" column="Message" type="string"/>
        <property name="Timestamp" column="Timestamp" type="DateTime"/>
        <property name="UserName" column="UserName" type="string"/>
        <property name="UserIP" column="UserIP" type="string"/>
        <property name="Source" column="Source" type="string"/>
        <property name="ScriptName" column="ScriptName" type="string"/>
        <property name="PostString" column="PostString" type="string"/>
    </class>
</hibernate-mapping>
