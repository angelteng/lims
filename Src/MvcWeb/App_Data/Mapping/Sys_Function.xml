<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Hope.ITMS.Model" assembly="Hope.ITMS.Model" auto-import="false">
    <class name="SysFunction" table="Sys_Function">
        <id name="ID" type="Int32">
            <column name="ID" sql-type="int" />
            <generator class="native"/>
        </id>
        <property name="ModuleID" column="ModuleID" type="Int32"/>
        <property name="FunctionKey" column="FunctionKey" type="string"/>
        <property name="Value" column="Value" type="Int32"/>
        <property name="FunctionName" column="FunctionName" type="string"/>
        <property name="FunctionUrl" column="FunctionUrl" type="string"/>
        <property name="AdminPage" column="AdminPage" type="Boolean"/>
        <property name="Remark" column="Remark" type="string"/>
    </class>
</hibernate-mapping>
