<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Hope.ITMS.Model" assembly="Hope.ITMS.Model" auto-import="false">
    <class name="SysModule" table="Sys_Module">
        <id name="ID" type="Int32">
            <column name="ID" sql-type="int" />
            <generator class="native"/>
        </id>
        <property name="ParentID" column="ParentID" type="Int32"/>
        <property name="ModuleName" column="ModuleName" type="string"/>
        <property name="DefaultUrl" column="DefaultUrl" type="string"/>
        <property name="OrderID" column="OrderID" type="Int32"/>
        <property name="Remark" column="Remark" type="string"/>
        <property name="IsActive" column="IsActive" type="Boolean"/>
    </class>
</hibernate-mapping>
